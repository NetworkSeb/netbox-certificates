BEGIN:VCALENDAR
VERSION:2.0
PRODID:Netbox Certificates - https://github.com/NetworkSeb/netbox-certificates/
{% for cert in certificates %}
BEGIN:VEVENT
UID:{{ cert.ca_reference }}
DTSTART;VALUE=DATE:{{ cert.expiry_date|date:"Ymd" }}
SUMMARY:{{ cert.certificate.cn }} expires
DESCRIPTION:Certificate {{ cert.certificate.cn }} expires on {{ cert.expiry_date|date:"Ymd" }}
END:VEVENT
{% endfor %}
END:VCALENDAR